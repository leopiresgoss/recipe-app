<nav>
  <div>
    <%= link_to root_path, { title: "Home" } do%>
      <%= image_tag "recipeapp-logo.svg", class: "logo" %>
    <% end %>
  </div>

  <div class="nav-links">
    <ul>
      <% if current_user %>
        <%= render "navbar/logged_user_links" %>
      <% else %>
        <li>
          <%= link_to 'Sign In', new_user_session_path, class: "nav-link" %>
        </li>
        <li>
          <%= link_to 'Sign Up', new_user_registration_path, class: "nav-link" %>
        </li>
      <% end %>
    </ul>
  </div>
</nav>